#!/usr/bin/env node

var koa = require('koa');
var render = require('koa-ejs');
var path = require('path');

var app = koa();

render(app, {
  root: 'view',
  layout: 'template',
  viewExt: 'html',
  cache: false,
  debug: true
});

app.use(function *(){
  yield this.render('test');
});

app.listen(3000);